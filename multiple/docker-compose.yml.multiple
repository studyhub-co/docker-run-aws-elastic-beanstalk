version: '2.4'
services:
  docker-run:
    image: "glot/docker-run:latest"
    volumes:
      - "/var/run/docker.sock:/var/run/docker.sock"
    ports:
      - "8088:8088"
    environment:
      API_ACCESS_TOKEN: replace-with-deploy
  create-react-app:
    image: "vermusl/create-react-app:latest"
    restart: "no"
    # pull: true
